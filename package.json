{
  "name": "vue-i18n",
  "description": "Internationalization plugin for Vue.js",
  "version": "9.0.0-alpah.0",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/inlitify/vue-i18n/issues"
  },
  "dependencies": {
    "vue": "^3.0.0-alpha.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/jest": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@typescript-eslint/typescript-estree": "^2.14.0",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue-libs": "^4.0.0",
    "jest": "^25.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "opener": "^1.5.1",
    "prettier": "^1.19.1",
    "rollup": "^1.32.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.8.3",
    "typescript-eslint-language-service": "^2.0.3"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/intlify/vue-i18n#readme",
  "keywords": [
    "i18n",
    "internationalization",
    "plugin",
    "vue",
    "vue.js"
  ],
  "license": "MIT",
  "main": "dist/vue-i18n.cjs.js",
  "module": "dist/vue-i18n.esm.bundler.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/vue-i18n.git"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "build:watch": "tsc -p . --watch",
    "clean": "npm run clean:dist && npm run clean:cache && npm run clean:coverage",
    "clean:dist": "rm -rf ./dist/**",
    "clean:cache": "rm -rf ./node_modules/.rts2_cache",
    "clean:coverage": "rm -rf ./coverage",
    "coverage": "opener coverage/lcov-report/index.html",
    "lint": "eslint ./src ./test --ext .ts",
    "test": "npm run lint && npm run test:cover",
    "test:cover": "npm run test:unit -- --coverage",
    "test:type": "tsc -p .",
    "test:unit": "jest --env node",
    "test:watch": "jest --env node --watch"
  },
  "sideEffects": false,
  "types": "types/index.d.ts",
  "unpkg": "dist/vue-i18n.iife.js"
}
